#include <stdio.h>
#include <stdlib.h>
#define MAX 100

struct stack {
  float elem[MAX];
  int top;
};

void init(struct stack *stk) {
  stk->top = 0;
}

void push(struct stack *stk, float f) {
	
  if(stk->top < MAX) {
    stk->elem[stk->top] = f;
    stk->top++;
  } else
    printf("error\n", stk->top);
    
}

float pop(struct stack *stk) {
  float elem;
  
  if((stk->top) > 0)
  {
    stk->top--;
    
    elem = stk->elem[stk->top];
    
    return elem;
  }
  else
  {
  	
    printf("pustoi!\n");
    
    return 0;
  }
}

int isempty(struct stack *stk) {
	
  if(stk->top == 0)    
  return 1;
  
  else 
  return 0;
  
}

void stkPrint(struct stack *stk) {
  int i;
  
  i=stk->top; // i - количество элементов в стеке
  
  if(isempty(stk) == 1) return; // стек пуст
  
  do {
  	
    i--;
    printf("%f\n", stk->elem[i]);
    
  } while(i>0);
}

int main() {
  struct stack *stk;
  int i,n,v;
  float elem;
  
  stk = (struct stack*)malloc(sizeof(struct stack));
  init(stk);
  
  	while(v!=4){
		printf("1)ADD\n2)DELETE\n3)PRINT\n");
  		scanf("%d", &v);
  
  switch(v){
			case 1:
			    printf("enter element:");
    			scanf("%f", &elem);
    			push(stk,elem);
			break;
			
			case 2:
			    printf("delete %f\n", pop(stk));	
			break;
			
			case 3:
			    stkPrint(stk);
			break;
			
			case 4:
				exit(0);
			break;
  
		}

	}
	
	return 0;
	
}
